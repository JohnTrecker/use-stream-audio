{
  "name": "use-stream-audio",
  "version": "1.0.0",
  "description": "React hook for streaming audio playback using Web Audio API and HTML5 Audio",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "hooks",
    "audio",
    "streaming",
    "web-audio-api",
    "html5-audio",
    "playback"
  ],
  "author": "John Trecker <nosautem@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "audiobuffer-to-wav": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johntrecker/use-stream-audio.git"
  },
  "bugs": {
    "url": "https://github.com/johntrecker/use-stream-audio/issues"
  },
  "homepage": "https://github.com/johntrecker/use-stream-audio#readme"
}
